{
  "build": {
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "productName": "jgui",
  "version": "0.1.0",
  "identifier": "com.jules.jgui",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "JGUI",
        "width": 900,
        "height": 600,
        "visible": true,
        "center": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.png"]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}